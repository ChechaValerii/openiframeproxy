{
    "extends": "airbnb",
    "rules": {
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-underscore-dangle": 0,
        "newline-after-var": 2,
        "func-names": 0,
        "no-param-reassign": 0,
        "no-console": 0,
        "consistent-return": 0,
        "no-alert": 0,
        "no-shadow": 0,
        "comma-dangle": ["error", {
            "functions": "ignore"
        }],
        "max-len": ["error", {
            "code": 150
        }],
        "no-trailing-spaces": 0,
        "newline-before-return": "error",
        "no-bitwise": ["error", {
            "allow": ["~"]
        }],
        "no-unused-expressions": 0,
        "no-plusplus": 0,
        "object-shorthand": 0,
        "arrow-body-style": 0,
        "no-prototype-builtins": 0,
        "function-paren-newline": ["error", "consistent"],
        "arrow-parens": [2, "always"],
        "no-restricted-syntax": 0,
        "new-cap": [
            "error",
            {
                "properties": false
            }
        ]
    },
    "globals": {
        "document": false,
        "parent": false,
        "window": false
    }
}